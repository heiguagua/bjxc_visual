<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.chinawiserv.dsp.dir.mapper.cs.CsDataSyncCollectBlockMapper">
    <!-- CsDataSyncCollectBlock的resultMap,column是给数据库列起的别名,它对应property类的属性-->
    <resultMap id="result_CsDataSyncCollectBlock_Map"
               type="com.chinawiserv.dsp.dir.entity.po.cs.CsDataSyncCollectBlock">
        <id column="id" property="id"/>
        <result column="block_name" property="blockName"/>
        <result column="block_url" property="blockUrl"/>
        <result column="table_id" property="tableId"/>
        <result column="website_name" property="websiteName"/>
    </resultMap>

    <!-- 数据库中表名为:cs_data_sync_collect_block的列名,as前是数据库的列明,as后是列的别名用于映射成实体类中的属性,需要注意的是别名必须与resultMap中的column别名一致 -->
    <sql id="cs_data_sync_collect_block_Column">
        cs_data_sync_collect_block.id as id
        ,cs_data_sync_collect_block.block_name as block_name
        ,cs_data_sync_collect_block.block_url as block_url
        ,cs_data_sync_collect_block.table_id as table_id
        ,cs_data_sync_collect_block.website_name as website_name
    </sql>


    <!-- 获得一个CsDataSyncCollectBlock对象,以参数CsDataSyncCollectBlock对象中不为空的属性作为条件进行查询-->
    <select id="selectCsDataSyncCollectBlockByObj"
            parameterType="com.chinawiserv.dsp.dir.entity.po.cs.CsDataSyncCollectBlock"
            resultMap="result_CsDataSyncCollectBlock_Map">
        select
        <include refid="cs_data_sync_collect_block_Column"/>
        from cs_data_sync_collect_block
        <where>
            <if test="websiteName != null ">and cs_data_sync_collect_block.website_name = #{websiteName}</if>
            <if test="tableId != null ">and cs_data_sync_collect_block.table_id = #{tableId}</if>
            <if test="blockUrl != null ">and cs_data_sync_collect_block.block_url = #{blockUrl}</if>
            <if test="blockName != null ">and cs_data_sync_collect_block.block_name = #{blockName}</if>
            <if test="id != null ">and cs_data_sync_collect_block.id = #{id}</if>
        </where>
    </select>

    <!-- 通过CsDataSyncCollectBlock的id获得对应数据库中表的数据对象-->
    <select id="selectCsDataSyncCollectBlockById" parameterType="java.lang.String"
            resultMap="result_CsDataSyncCollectBlock_Map">
        select
        <include refid="cs_data_sync_collect_block_Column"/>
        from cs_data_sync_collect_block
        where cs_data_sync_collect_block.id = #{id}
    </select>

    <!-- 将CsDataSyncCollectBlock插入到对应数据库的表中,包括属性值为null的数据-->

    <insert id="insertCsDataSyncCollectBlock"
            parameterType="com.chinawiserv.dsp.dir.entity.po.cs.CsDataSyncCollectBlock">
        insert into cs_data_sync_collect_block(id,block_name,block_url,table_id,website_name) 
        values(#{id},#{blockName},#{blockUrl},#{tableId},#{websiteName})
    </insert>

    <!-- 将CsDataSyncCollectBlock中属性值不为null的数据,插入到对应数据库的表中-->
    <insert id="insertNonEmptyCsDataSyncCollectBlock"
            parameterType="com.chinawiserv.dsp.dir.entity.po.cs.CsDataSyncCollectBlock">
        insert into cs_data_sync_collect_block
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="id != null">id,</if>
            <if test="blockName != null">block_name,</if>
            <if test="blockUrl != null">block_url,</if>
            <if test="tableId != null">table_id,</if>
            <if test="websiteName != null">website_name,</if>
        </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
            <if test="id != null">#{id},</if>
            <if test="blockName != null">#{blockName},</if>
            <if test="blockUrl != null">#{blockUrl},</if>
            <if test="tableId != null">#{tableId},</if>
            <if test="websiteName != null">#{websiteName},</if>
        </trim>
    </insert>

    <!-- 通过CsDataSyncCollectBlock的id将CsDataSyncCollectBlock的数据更新到数据库中对应的表,包括值null的数据-->
    <update id="updateCsDataSyncCollectBlockById"
            parameterType="com.chinawiserv.dsp.dir.entity.po.cs.CsDataSyncCollectBlock">
        update cs_data_sync_collect_block set
            block_name=#{blockName}
            ,block_url=#{blockUrl}
            ,table_id=#{tableId}
            ,website_name=#{websiteName}
        where id=#{id}
    </update>

    <!-- 通过CsDataSyncCollectBlock的id将CsDataSyncCollectBlock中属性值不为null的数据更新到数据库对应的表中-->
    <update id="updateNonEmptyCsDataSyncCollectBlockById"
            parameterType="com.chinawiserv.dsp.dir.entity.po.cs.CsDataSyncCollectBlock">
        update cs_data_sync_collect_block
        <set>
            <if test="blockName != null">
                block_name=#{blockName},
            </if>
            <if test="blockUrl != null">
                block_url=#{blockUrl},
            </if>
            <if test="tableId != null">
                table_id=#{tableId},
            </if>
            <if test="websiteName != null">
                website_name=#{websiteName},
            </if>
        </set>
        where id=#{id}
    </update>

</mapper>