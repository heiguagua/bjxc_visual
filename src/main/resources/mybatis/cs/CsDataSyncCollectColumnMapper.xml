<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.chinawiserv.dsp.dir.mapper.cs.CsDataSyncCollectColumnMapper">
    <!-- CsDataSyncCollectColumn的resultMap,column是给数据库列起的别名,它对应property类的属性-->
    <resultMap id="result_CsDataSyncCollectColumn_Map"
               type="com.chinawiserv.dsp.dir.entity.po.cs.CsDataSyncCollectColumn">
        <id column="id" property="id"/>
        <result column="cloumn_name" property="cloumnName"/>
        <result column="cloumn_chzn" property="cloumnChzn"/>
        <result column="table_id" property="tableId"/>
    </resultMap>

    <!-- 数据库中表名为:cs_data_sync_collect_column的列名,as前是数据库的列明,as后是列的别名用于映射成实体类中的属性,需要注意的是别名必须与resultMap中的column别名一致 -->
    <sql id="cs_data_sync_collect_column_Column">
        cs_data_sync_collect_column.id as id
        ,cs_data_sync_collect_column.cloumn_name as cloumn_name
        ,cs_data_sync_collect_column.cloumn_chzn as cloumn_chzn
        ,cs_data_sync_collect_column.table_id as table_id
    </sql>


    <!-- 获得一个CsDataSyncCollectColumn对象,以参数CsDataSyncCollectColumn对象中不为空的属性作为条件进行查询-->
    <select id="selectCsDataSyncCollectColumnByObj"
            parameterType="com.chinawiserv.dsp.dir.entity.po.cs.CsDataSyncCollectColumn"
            resultMap="result_CsDataSyncCollectColumn_Map">
        select
        <include refid="cs_data_sync_collect_column_Column"/>
        from cs_data_sync_collect_column
        <where>
            <if test="tableId != null ">and cs_data_sync_collect_column.table_id = #{tableId}</if>
            <if test="cloumnChzn != null ">and cs_data_sync_collect_column.cloumn_chzn = #{cloumnChzn}</if>
            <if test="cloumnName != null ">and cs_data_sync_collect_column.cloumn_name = #{cloumnName}</if>
            <if test="id != null ">and cs_data_sync_collect_column.id = #{id}</if>
        </where>
    </select>

    <!-- 通过CsDataSyncCollectColumn的id获得对应数据库中表的数据对象-->
    <select id="selectCsDataSyncCollectColumnById" parameterType="java.lang.String"
            resultMap="result_CsDataSyncCollectColumn_Map">
        select
        <include refid="cs_data_sync_collect_column_Column"/>
        from cs_data_sync_collect_column
        where cs_data_sync_collect_column.id = #{id}
    </select>

    <!-- 将CsDataSyncCollectColumn插入到对应数据库的表中,包括属性值为null的数据-->

    <insert id="insertCsDataSyncCollectColumn"
            parameterType="com.chinawiserv.dsp.dir.entity.po.cs.CsDataSyncCollectColumn">
        insert into cs_data_sync_collect_column(id,cloumn_name,cloumn_chzn,table_id) 
        values(#{id},#{cloumnName},#{cloumnChzn},#{tableId})
    </insert>

    <!-- 将CsDataSyncCollectColumn中属性值不为null的数据,插入到对应数据库的表中-->
    <insert id="insertNonEmptyCsDataSyncCollectColumn"
            parameterType="com.chinawiserv.dsp.dir.entity.po.cs.CsDataSyncCollectColumn">
        insert into cs_data_sync_collect_column
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="id != null">id,</if>
            <if test="cloumnName != null">cloumn_name,</if>
            <if test="cloumnChzn != null">cloumn_chzn,</if>
            <if test="tableId != null">table_id,</if>
        </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
            <if test="id != null">#{id},</if>
            <if test="cloumnName != null">#{cloumnName},</if>
            <if test="cloumnChzn != null">#{cloumnChzn},</if>
            <if test="tableId != null">#{tableId},</if>
        </trim>
    </insert>

    <!-- 通过CsDataSyncCollectColumn的id将数据库表中对应的数据删除-->
    <delete id="deleteCsDataSyncCollectColumnById" parameterType="java.lang.String">
        delete from cs_data_sync_collect_column
        where id = #{id}
    </delete>

    <!-- 通过CsDataSyncCollectColumn的id将CsDataSyncCollectColumn的数据更新到数据库中对应的表,包括值null的数据-->
    <update id="updateCsDataSyncCollectColumnById"
            parameterType="com.chinawiserv.dsp.dir.entity.po.cs.CsDataSyncCollectColumn">
        update cs_data_sync_collect_column set
            cloumn_name=#{cloumnName}
            ,cloumn_chzn=#{cloumnChzn}
            ,table_id=#{tableId}
        where id=#{id}
    </update>

    <!-- 通过CsDataSyncCollectColumn的id将CsDataSyncCollectColumn中属性值不为null的数据更新到数据库对应的表中-->
    <update id="updateNonEmptyCsDataSyncCollectColumnById"
            parameterType="com.chinawiserv.dsp.dir.entity.po.cs.CsDataSyncCollectColumn">
        update cs_data_sync_collect_column
        <set>
            <if test="cloumnName != null">
                cloumn_name=#{cloumnName},
            </if>
            <if test="cloumnChzn != null">
                cloumn_chzn=#{cloumnChzn},
            </if>
            <if test="tableId != null">
                table_id=#{tableId},
            </if>
        </set>
        where id=#{id}
    </update>

</mapper>